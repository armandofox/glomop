@%!PS-Adobe-2.0 EPSF-2.0
%%Title: chart.ps
%%Creator: Ian Goldberg
%%CreationDate: Wed Dec 13 01:51:14 PST 1995
%%For: iang@@cs.berkeley.edu
%%Orientation: Portrait
%%BoundingBox: -7.5 -7.5 30 30
%%Pages: 0
%%EndComments
/doborder
{
    /scl exch def
    /ticksize exch def
    /ticks exch def
    /size exch def
    /ycap exch def
    /xcap exch def
    size 0 moveto
    0 0 lineto
    0 size lineto
    stroke
    0 ticks size
    {
	dup
	dup dup
	dup
	dup
	0 moveto
	0 ticksize rlineto stroke
	0 moveto
	(xxxx) cvs dup stringwidth pop 2 div neg scl neg rmoveto show
	0 exch moveto
	ticksize 0 rlineto stroke
	0 exch moveto
	(xxxx) cvs dup stringwidth pop neg scl 2 div neg rmoveto
	( ) stringwidth pop neg 0 rmoveto show
    } for
    0 0 moveto
    size size lineto
    stroke
    size 2 div scl 2.5 mul neg moveto
    xcap dup stringwidth pop 2 div neg 0 rmoveto show
    gsave
    90 rotate
    size 2 div scl 2.5 mul moveto
    ycap dup stringwidth pop 2 div neg 0 rmoveto show
    grestore
} bind def

/dopoint
{
    /ticksize exch def
    /yrng exch def
    /ypt exch def
    /xrng exch def
    /xpt exch def
    xpt xrng sub ypt yrng sub moveto
    xrng 2 mul 0 rlineto
    0 yrng 2 mul rlineto
    xrng -2 mul 0 rlineto
    closepath stroke
    xpt ypt ticksize sub moveto
    0 ticksize 2 mul rlineto
    stroke
    xpt ticksize sub ypt moveto
    ticksize 2 mul 0 rlineto
    stroke
} bind def

/t
{
    30 40 div dopoint
} bind def

30 400 div setlinewidth
/Times-Roman findfont 30 20 div scalefont setfont

(Untraced ghostscript times (sec))
(Traced ghostscript times (sec))
30 5 30 80 div 30 20 div doborder
	1.1862	0.04275		1.1938	0.0419948 t
	1.5956	0.0499664	1.6362	0.0708206 t
	3.6472	0.0810442	3.719	0.141538 t
	4.4312	0.0741523	4.1796	0.0924978 t
	6.6766	0.103839	6.4752	0.144433 t
	9.4446	0.285371	9.1756	0.269527 t
	28.3788	0.626079	28.7554	0.788822 t

@


